<%- include('partials/header'); %>
<link rel="stylesheet" href="/css/styles.css">
<h1 class="title">Home</h1>
<p> <%= homeStart %></p>

<%  posts.forEach(function(post){ %>

<h1 class="title"><%=post.title%></h1>
